        "def check_length(text):\n",
        "  text = text.split()\n",
        "  contain = []\n",
        "  \n",
        "  for words in text:\n",
        "    if len(words) > 2:\n",
        "      contain.append(words)\n",
        "      \n",
        "  final = ' '.join(contain)\n",
        "  return final\n",
        "      \n",
        "  \n",
        "df['Text'] = df['Text'].apply(check_length)\n",
        "\n",
        "df.head()"
